#!/bin/sh
set -e
if test "$CONFIGURATION" = "Debug"; then :
  cd /Users/parsasedighi/Desktop/cisc187-sp24-fire29/project1-take-away/build
  /opt/homebrew/Cellar/cmake/3.28.2/bin/ctest --force-new-ctest-process -C $CONFIGURATION$EFFECTIVE_PLATFORM_NAME
fi
if test "$CONFIGURATION" = "Release"; then :
  cd /Users/parsasedighi/Desktop/cisc187-sp24-fire29/project1-take-away/build
  /opt/homebrew/Cellar/cmake/3.28.2/bin/ctest --force-new-ctest-process -C $CONFIGURATION$EFFECTIVE_PLATFORM_NAME
fi
if test "$CONFIGURATION" = "MinSizeRel"; then :
  cd /Users/parsasedighi/Desktop/cisc187-sp24-fire29/project1-take-away/build
  /opt/homebrew/Cellar/cmake/3.28.2/bin/ctest --force-new-ctest-process -C $CONFIGURATION$EFFECTIVE_PLATFORM_NAME
fi
if test "$CONFIGURATION" = "RelWithDebInfo"; then :
  cd /Users/parsasedighi/Desktop/cisc187-sp24-fire29/project1-take-away/build
  /opt/homebrew/Cellar/cmake/3.28.2/bin/ctest --force-new-ctest-process -C $CONFIGURATION$EFFECTIVE_PLATFORM_NAME
fi

